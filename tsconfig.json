{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": false,
    "outDir": "./",
    "noEmitOnError": false,
    "incremental": true,
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
